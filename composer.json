{
    "name": "ksuzuki2016/laravel-http-client",
    "type": "library",
    "description": "The Laravel Http Facade Expansion",
    "keywords": [
        "HttpClient",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.3 || >=8.0",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-zlib": "*",
        "ext-dom": "*",
        "ext-simplexml": "*",
        "symfony/dom-crawler": "^5.0",
        "php-webdriver/webdriver": "^1.8",
        "laravel/dusk": "^6.0 || ^5.0",
        "symfony/process": "^5.1",
        "guzzlehttp/guzzle": "^7.0 || ^6.0"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "mockery/mockery": "^1.4",
        "phpunit/phpunit": "^9.4",
        "orchestra/testbench-core": "^6.6",
        "vimeo/psalm": "^4.1",
        "orchestra/testbench": "^6.4"

    },
    "autoload": {
        "psr-4": {
            "KSuzuki2016\\HttpClient\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "KSuzuki2016\\HttpClient\\Tests\\": "tests"
        }
    },
     "extra": {
        "laravel": {
            "providers": [
                "KSuzuki2016\\HttpClient\\HttpClientServiceProvider"
            ]
        }
    }
}

