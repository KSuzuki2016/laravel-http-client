{
    "name": "KSuzuki2016/laravel-http-client",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-zlib": "*",
        "ext-dom": "*",
        "ext-simplexml": "*",
        "laravel/framework": "^7.0",
        "league/uri": "^6.2",
        "league/uri-query-parser": "^1.0",
        "spatie/url": "^1.3",
        "symfony/dom-crawler": "^5.0",
        "duncan3dc/dusk": "^1.0"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "HttpClient\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
     "extra": {
        "laravel": {
            "providers": [
                "HttpClient\\HttpClientServiceProvider"
            ],
            "aliases": {
                "Http": "Illuminate\\Support\\Facades\\Http"
            }
        }
    }
}
